   BEGINSUB Name=RunMacro
     OPTION Explicit=NO

     IF $_AltKey OR FileExists($MmMacro) OR At("?",$MmMacro)>0
       IF $_AltKey
         IF StrToBool(SysInfo("gsakini;TFMConfig;chkMacroEditor.Checked"))
           $MmEditor=SysInfo("gsakini;TFMConfig;edtMacroEditor.FileName")
         ELSE 
           $MmEditor="$_exePath\MacroEditor.exe"
         ENDIF 
         IF FileExists($MmMacro)
           RUNPGM Pgm=$MmEditor                 # Open the editor - the following delay should allow time to open other macros being edited
           DELAY MS=500                         # 1/2 second delay then
           RUNPGM Pgm=$MmEditor Parms=$MmMacro  # Pass the name of the macro to the editor
           RETURN # Close MenuMaster
         ELSE 
           $MmMsg="ERROR - The file does not exist"+$_Newline+"$MmMacro"
           MSGOK Msg=$MmMsg Caption="Somebody goofed"
         ENDIF 
       ELSE 
         IF $MmVscope AND $_Version >= "7.7.2.37"
           MACRO File=$MmMacro VarScope=Local
         ELSE 
           IF $MmVscope
             $MmMsg="This version of Gsak does not support local variables" + $_NewLine + "ALL variables should be considered GLOBAL"
             $MmMsg=$MmMsg+$_newline+$_newline+"To resolve this, update Gsak to v7.7.2.37 or later"
             MSGOK Msg=$MmMsg Caption="Please Note"
           ENDIF 
           MACRO File=$MmMacro
         ENDIF 
       ENDIF 
     ELSE 
       IF $MmMacro=""
         $MmMsg="There is no macro linked to this button"
       ELSE 
         $MmMsg="The macro linked to this button is not available for use."
       ENDIF 
       MSGOK Msg=$MmMsg
     ENDIF 

     OPTION Explicit=YES
     SHOWSTATUS Msg="" Display=OFF
   ENDSUB 
