   BEGINSUB Name=SwapPositions
# 1 - Create macro code dynamically. # 2 - Save the dynamic code to a macro file. # 3 - Run the new macro
     IF $MmSwapA <> "0" AND $MmSwapB<>"0"
       $MmFiledata=""
       
       $MmTempStr="$MmSwapA;$MmSwapB"
      
# 1a - Swap Button captions
       $MmFiledata=$MmFiledata+"$"+"MmTempCap=$"+"MmF2Bcap"+"$MmSwapA"+$_NewLine
       $MmFiledata=$MmFiledata+"$"+"MmF2Bcap"+"$MmSwapA=$"+"MmF2Bcap"+"$MmSwapB"+$_NewLine
       $MmFiledata=$MmFiledata+"$"+"MmF2Bcap"+"$MmSwapB=$"+"MmTempCap"+$_NewLine+$_NewLine
# 1b - Swap Description text
       $MmFiledata=$MmFiledata+"$"+"MmTempDesc=$"+"MmF2Desc"+"$MmSwapA"+$_NewLine
       $MmFiledata=$MmFiledata+"$"+"MmF2Desc"+"$MmSwapA=$"+"MmF2Desc"+"$MmSwapB"+$_NewLine
       $MmFiledata=$MmFiledata+"$"+"MmF2Desc"+"$MmSwapB=$"+"MmTempDesc"+$_NewLine+$_NewLine
# 1c - Swap File names
       $MmFiledata=$MmFiledata+"$"+"MmTempFile=$"+"MmF2File"+"$MmSwapA"+$_NewLine
       $MmFiledata=$MmFiledata+"$"+"MmF2File"+"$MmSwapA=$"+"MmF2File"+"$MmSwapB"+$_NewLine
       $MmFiledata=$MmFiledata+"$"+"MmF2File"+"$MmSwapB=$"+"MmTempFile"+$_NewLine+$_NewLine

       $MmSwapA=Right("0"+$MmSwapA,2)
       $MmSwapB=Right("0"+$MmSwapB,2)

       DECLARE Var=$MmTempColor Type=String
# 1d - Swap Color values ($MmMacCol**)
       $MmFiledata=$MmFiledata+"$"+"MmTempColor=$"+"MmMacCol"+"$MmSwapA"+$_NewLine
       $MmFiledata=$MmFiledata+"$"+"MmMacCol"+"$MmSwapA=$"+"MmMacCol"+"$MmSwapB"+$_NewLine
       $MmFiledata=$MmFiledata+"$"+"MmMacCol"+"$MmSwapB=$"+"MmTempColor"+$_NewLine+$_NewLine

       DECLARE Var=$MmTempStyle Type=String
# 1e - Swap Style values ($MmMacSt**)
       $MmFiledata=$MmFiledata+"$"+"MmTempStyle=$"+"MmMacStl"+"$MmSwapA"+$_NewLine
       $MmFiledata=$MmFiledata+"$"+"MmMacStl"+"$MmSwapA=$"+"MmMacStl"+"$MmSwapB"+$_NewLine
       $MmFiledata=$MmFiledata+"$"+"MmMacStl"+"$MmSwapB=$"+"MmTempStyle"+$_NewLine+$_NewLine

# 2 - Save $MmFiledata to a macro file
       $MmTempFname="$_AppData\Temp\TempMacro.Gsk"
       $MmTmpStr=PutFile($MmTempFname,$MmFiledata)
# 3 - Run the new macro
       MACRO File=$MmTempFname
# 4 - Apply the Color and Style changes then Restore changed values
       GOSUB Name=SetButtonCaptions
       GOSUB Name=SetLabelCaptions       
       GOSUB Name=SetLabelCS
       $MmSwapA=Extract($MmTempStr,";",1)
       $MmSwapB=Extract($MmTempStr,";",2)
     ENDIF 
   ENDSUB 
