   BEGINSUB Name=GetRgbColors                 # Return Red/Green/Blue decimal values
     IF Left($MmRgbColor,1)="#"               # $MmRgbColor must begin with "#"
       $MmHex=Substr($MmRgbColor,2,2)
       GOSUB Name=hextodec
       $MmRed=$MmDec
       $MmHex=Substr($MmRgbColor,4,2)
       GOSUB Name=hextodec
       $MmGreen=$MmDec
       $MmHex=Substr($MmRgbColor,6,2)
       GOSUB Name=hextodec
       $MmBlue=$MmDec
       $MmNewColor=NumtoStr(RGBColor($MmRed,$MmGreen,$MmBlue))
     ENDIF 
   ENDSUB 
###################################################################################
   BEGINSUB Name=DecToRGB                          # Mod - is the remainder of the division. Div - is the integer part of the result of the division.
     $$MmVarDec=Val($MmDecimal)
     $MmResultStr=""
     $MmHTDSUBhexcodes="0123456789ABCDEF"
     WHILE $$MmVarDec>16
       $Mmx=Int($$MmVarDec/16)
       $Mmy=$$MmVarDec-($Mmx*16)
       $MmResultStr=$MmResultStr+Substr($MmHTDSUBhexcodes,$Mmy+1,1)
       $$MmVarDec=$Mmx
      ENDWHILE 
     $MmResultStr=$MmResultStr+Substr($MmHTDSUBhexcodes,$Mmx+1,1)
     $MmResultStr="#"+Left($MmResultStr+"000000",6)
     $MmMacColor=$MmResultStr
   ENDSUB 
###################################################################################
   BEGINSUB Name=hextodec
     $MmHTDSUBnumDigits=len($MmHex)
     IF $MmHTDSUBnumDigits = 0
       $MmDec=-1                         # an error condition. no hex characters were passed
     ELSE 
       $MmHex=Upper($MmHex)
       $MmDec=0
       $MmHTDSUBhexcodes="0123456789ABCDEF"
       $MmHTDSUBcounter=0
       WHILE $MmHTDSUBcounter < $MmHTDSUBnumDigits
         $MmDec=16*$MmDec + (AT(Substr($MmHex, $MmHTDSUBcounter, 1), $MmHTDSUBhexcodes)-1)
         $MmHTDSUBcounter = $MmHTDSUBcounter + 1
       ENDWHILE 
     ENDIF 
   ENDSUB #hextodec
