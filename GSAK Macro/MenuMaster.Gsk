     IF Left($MmResultStr ,4) <> "*OK*"
       PAUSE Msg="Sorry! For some reason I couldn't generate the colorpicker file"
       CANCEL 
     ELSE 
       $MmFolder = $_AppData + "\Temp\"
       $MmResultStr = ZipFile("unzip",$MmFileName,$MmFolder)
       $MmUrl="file:///" + $_AppData + "\Temp\colorpicker.html"
     ENDIF 
