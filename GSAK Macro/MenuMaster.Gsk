   BEGINSUB Name=ReCenterLabel
#    Get the main Form width
     $MmCcontrol = RegExEscape("MmForm1")
     $MmProperty = RegExEscape("Width")
     $MmValue  = RegExSub("(?s)$MmCcontrol.*?$MmProperty.*?=(.*?\r\n)",$MmFormA,1,1)
     $MmFwidth  = Val(AllTrim($MmValue))
#    Get the Form Id label width
     $MmCcontrol = RegExEscape("MmLbl100")
     $MmProperty = RegExEscape("Width")
     $MmValue = RegExSub("(?s)$MmCcontrol.*?$MmProperty.*?=(.*?\r\n)",$MmFormA,1,1)
     $MmLblWidth  = Val(AllTrim($MmValue))
#    Calculate a new position for the labels left edge and reposition it.
     $MmNewLeft = NumToStr(Int(($MmFwidth/2) - 6.65*(Len($MmMfName))))
     $MmFormA=EditForm($MmFormA,"MmLbl100","Left",$MmNewLeft)
   ENDSUB 
