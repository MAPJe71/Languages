   BEGINSUB Name=DisplayCloneForm
     $MmFormC=EditForm($MmFormC,"MmCurrentPrefix","Caption",$MmPrefix)
     $MmFileList=""
     GOSUB Name=ReadCtlFile
     WHILE TRUE
       $MmFormExit = Form($MmFormC,"")
       BEGINCASE 
         CASE $mmFormExit="MmPrefixCheck"
           GOSUB Name=CheckPrefix
         CASE $MmFormExit="MmExitWith"
           IF $MmNewPrefix=""
             MSGOK Msg="You must enter a Variable Prefix for the new macro"
           ELSE 
             IF FolderExists($MmNewFolder) AND $MmNewName<>""
               IF Lower(Right($MmNewName,4))<>".Gsk"
                 $MmNewName=$MmNewName+".Gsk"
               ENDIF 
               $MmFullName="$MmNewFolder\$MmNewName"
               IF At($MmFullName,$MmFileList)=0
                 $MmCntr=1
                 $MmCntr2=1
                 GOSUB Name=CloneFile
                 $MmFileList=$MmFileList+"$MmNewFolder;$MmNewName;$MmFullName;$MmNewPrefix;$MmvV;$_NewLine"
                 GOSUB Name=WriteCtlFile
                 SHOWSTATUS Msg="" Display=OFF
                 MSGOK Msg="MenuMaster has been cloned to $MmFullName ($MmNewPrefix)"
               ELSE 
                 MSGOK Msg="Sorry - this filename already exists - please choose a different name"
               ENDIF 
             ELSE 
               MSGOK Msg="Please check the Folder name and/or the File name"
             ENDIF 
             BREAK 
           ENDIF 
         CASE $MmFormExit="MmExitWithout"
           BREAK 
       ENDCASE 
     ENDWHILE 
   ENDSUB 
###################################################################################
   BEGINSUB Name=CheckPrefix
     IF $MmNewPrefix<>""
       IF Lower($MmNewPrefix) <> "child"
         IF Lower($MmNewPrefix)=Lower($MmPrefix) OR RegEx(RegexEscape(";$MmNewPrefix;"),$MmFileList)
           MSGOK Msg="This prefix has been used previously - use it with caution"
         ELSE 
           MSGOK Msg="This prefix is Ok to use."
         ENDIF 
       ELSE 
         MSGOK Msg="Sorry - this prefix is Reserved"
       ENDIF 
     ELSE 
       MSGOK Msg="Sorry - You must have a variable prefix."
     ENDIF 
   ENDSUB 
###################################################################################
   BEGINSUB Name=CloneFile
     $MmOldMacro=Sysinfo("MacroPath")+"\"+Sysinfo("MacroFile")
     $MmMacData=GetFile($MmOldMacro)
#        $MmMacData=Replace($MmPrefix,$MmNewPrefix,$MmMacData)
     $MmMacData=""
     $MmDataStatement=FALSE
     SHOWSTATUS Msg="Processing $MmNewName  ($MmCntr2 of $MmCntr)" Width=350
     FILEREAD File=$MmOldMacro
       IF NOT($MmDataStatement)
         $line=Replace($MmPrefix,$MmNewPrefix,$line)
       ENDIF 
       IF RegEx("Data",$line)                                         # Moved down from before 'IF NOT($MmDataStatement)' block
         IF RegEx("VarName",$line)
           IF RegEx("colorpicker",$line)
             $MmDataStatement=TRUE
           ENDIF 
         ENDIF 
       ENDIF 
       $MmMacData=$MmMacData+$line+$_NewLine
       IF $MmDataStatement AND RegEx(RegexEscape("<EndData>"),$line)
         $MmDataStatement=FALSE
       ENDIF 
     ENDREAD 
     $MmMacData=Replace($MmvN,$MmNewName,$MmMacData)
     $MmResultStr=PutFile($MmFullName,$MmMacData)
   ENDSUB 
###################################################################################
   BEGINSUB Name=MacroUpdater            # Note: MmUpdater button is only displayed on MenuMaster.Gsk
     $MmUpdatedFileList=""
     IF FileExists($MmCtlFile)
       $MmMsg="Re-Clone (update) all Child macros to this version;"
       $MmMsg1="Continue and Update all Child Macros"
       $MmMsg2="Cancel and exit"
       CHOOSE MmMsg=$MmMsg Opt1=$MmMsg1 Opt2=$MmMsg2
       IF $MmResult=2
         EXITSUB 
       ENDIF 
       $MmFileList=""
       GOSUB Name=ReadCtlFile
       $MmCntr=RegExCount($_NewLine,$MmFileList)
       $MmCntr2=1
       WHILE $MmCntr2 <> $MmCntr
         $MmCntr2=$MmCntr2+1
         $MmData       =Extract($MmFileList,$_NewLine,$MmCntr2)
         $MmNewFolder  =Extract($MmData,";",1)                      # Folder to write new file to
         $MmNewName      =Extract($MmData,";",2)                      # New file name
         $MmFullName   =Extract($MmData,";",3)                      # Fully qualified file name
         $MmNewPrefix  =Extract($MmData,";",4)                      # Prefix for all variables
         $MmFileVersion=Extract($MmData,";",5)                      # Version of the file being tested
         IF FileExists($MmFullName)
           IF Val($MmFileVersion) < Val($MmvV)
             $MmTempStr=$MmNewName
             GOSUB Name=CloneFile
             $MmNewFileList=$MmNewFileList+"$MmNewFolder;$MmNewName;$MmFullName;$MmNewPrefix;$MmvV;$_NewLine"
             $MmUpdatedFileList=$MmUpdatedFileList+$MmFullName+$_NewLine
           ELSE 
             $MmNewFileList=$MmNewFileList+$MmData+$_NewLine
           ENDIF 
         ENDIF 
       ENDWHILE 
       $MmFileList=$MmNewFileList
       GOSUB Name=WriteCtlFile
       SHOWSTATUS Msg="" Display=OFF
       IF $MmUpdatedFileList<>""
         $MmMsg="The following macros have been updated to version $MmvV $_NewLine$_NewLine"+$MmUpdatedFileList
         MSGOK Msg=$MmMsg
       ENDIF 
     ENDIF 
   ENDSUB 
